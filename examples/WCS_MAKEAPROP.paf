# Analysis Property
INSTR:       HSC       #INSTRUMENT_HSC_or_SC
MODE_CR:     PIX       #AUTO_for_AUTO_MODE,_PIX_and_VAL_for_MANUAL_MODE
MODE_CCDPOS: true      #1=DETERMINING_CCD_POSITION
PRECISION_POS: 0.02    #PRECISION_OF_DETERMINATION_CCD_POSITION(pixel)
ORDER_ASIP:  9         #SIP_ORDER_of_A__and_B
ORDER_PSIP:  9         #SIP_ORDER_of_AP_and_BP
MODE_REJ:    true      #REJECT_BAD_REFERENCES
CLIPSIGMA:   2.5       #STRENGTH_OF_REJECTION
CRPIX1:      000.000   #CRPIX_VALUE_OF_AXIS_1(MANUAL_MODE)
CRPIX2:      000.000   #CRPIX_VALUE_OF_AXIS_2(MANUAL_MODE)
CRVAL1:      72.5000   #CRPIX_VALUE_OF_AXIS_1(MANUAL_MODE)
CRVAL2:      12.0000   #CRPIX_VALUE_OF_AXIS_2(MANUAL_MODE)
FLAG_STD:    2         #0:non_output,_1:STEP_output,_2:detail_output
FLAG_OUT:    1         #OUTPUT_FLAG_for_engineering_test_(1=OUTPUT_fits_file)
DIR_OUT:     ./output    #OUTPUT_DIRECTORY_for_engineering_test
